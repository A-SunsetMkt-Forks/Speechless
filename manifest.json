{
    "manifest_version": 3,
    "name": "Speechless",
    "version": "0.1",
    "description": "把新浪微博的内容导出成 PDF 文件进行备份",
    
    "content_scripts": [
        {
          "js": ["scripts/jquery-3.6.1.min.js","scripts/content.js"],
          "css":["style/speechless.css"],
          "matches": [
            "https://weibo.com/*"
          ],
          "run_at": "document_end"
        }
      ],
      "web_accessible_resources": [{
        "resources": ["scripts/script.js"],
        "matches": ["<all_urls>"]
      }]
  }